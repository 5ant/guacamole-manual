    <refnamediv>
        <refname>curve</refname>
        <refpurpose>Adds the specified cubic bezier curve subpath.</refpurpose>
    </refnamediv>

    <refsynopsisdiv>

        <title>Synopsis</title>

        <para>
            <command>
                curve
                <parameter>LAYER</parameter>
                <parameter>CP1X</parameter>
                <parameter>CP1Y</parameter>
                <parameter>CP2X</parameter>
                <parameter>CP2Y</parameter>
                <parameter>X</parameter>
                <parameter>Y</parameter>
            </command>
        </para>

    </refsynopsisdiv>

    <refsection>
        <title>Description</title>
        <para>Adds the specified cubic bezier curve subpath.</para>
    </refsection>

    <refsection>

        <title>Parameters</title>

        <variablelist>
