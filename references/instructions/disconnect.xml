<refentry id="disconnect-instruction" 
    xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
    xmlns:xi="http://www.w3.org/2001/XInclude">

    <indexterm>
        <primary>disconnect</primary>
    </indexterm>

    <refmeta>
        <refentrytitle>disconnect</refentrytitle>
    </refmeta>

    <refnamediv>
        <refname>disconnect</refname>
        <refpurpose>Notifies the server that the connection is about to be closed by the client.</refpurpose>
    </refnamediv>

    <refsynopsisdiv>

        <title>Synopsis</title>

        <para>
            <command>
                disconnect
            </command>
        </para>

    </refsynopsisdiv>

    <refsection>
        <title>Description</title>
        <para>Notifies the server that the connection is about to be closed by the client. This message can be sent by the client during any phase, and takes no parameters.</para>
    </refsection>

    <refsection>

        <title>Parameters</title>

        <variablelist>
        </variablelist>

    </refsection>

