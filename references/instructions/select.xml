<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE refentry PUBLIC "-//OASIS//DTD DocBook V5.0//EN"
    "http://docbook.org/xml/5.0/dtd/docbook.dtd">

<refentry id="select-instruction" 
    xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
    xmlns:xi="http://www.w3.org/2001/XInclude">

    <indexterm>
        <primary>select</primary>
    </indexterm>

    <refmeta>
        <refentrytitle>select</refentrytitle>
    </refmeta>

    <refnamediv>
        <refname>select</refname>
        <refpurpose>Requests that the connection be made using the specified protocol.</refpurpose>
    </refnamediv>

    <refsynopsisdiv>

        <title>Synopsis</title>

        <para>
            <command>
                select
                <parameter>PROTOCOL</parameter>
            </command>
        </para>

    </refsynopsisdiv>

    <refsection>
        <title>Description</title>
        <para>Requests that the connection be made using the specified protocol. This message can be sent by the client during the connect phase only.</para>
    </refsection>

    <refsection>

        <title>Parameters</title>

        <variablelist>
            <varlistentry>
                 <term><parameter>PROTOCOL</parameter></term>
                 <listitem><para></para></listitem>
             </varlistentry>
        </variablelist>

    </refsection>

    <refsection>

        <title>Example</title>

        <programlisting>[example]</programlisting>

    </refsection>

</refentry>