<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE refentry PUBLIC "-//OASIS//DTD DocBook V5.0//EN"
    "http://docbook.org/xml/5.0/dtd/docbook.dtd">

<refentry id="arc-instruction" 
    xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
    xmlns:xi="http://www.w3.org/2001/XInclude">

    <indexterm>
        <primary>arc</primary>
    </indexterm>

    <refmeta>
        <refentrytitle>arc</refentrytitle>
    </refmeta>

    <refnamediv>
        <refname>arc</refname>
        <refpurpose>Adds the specified arc subpath.</refpurpose>
    </refnamediv>

    <refsynopsisdiv>

        <title>Synopsis</title>

        <para>
            <command>
                arc
                <parameter>LAYER</parameter>
                <parameter>X</parameter>
                <parameter>Y</parameter>
                <parameter>RADIUS</parameter>
                <parameter>START</parameter>
                <parameter>END</parameter>
                <parameter>NEGATIVE</parameter>
            </command>
        </para>

    </refsynopsisdiv>

    <refsection>
        <title>Description</title>
        <para>Adds the specified arc subpath.</para>
    </refsection>

    <refsection>

        <title>Example</title>

        <programlisting>[example]</programlisting>

    </refsection>

</refentry>