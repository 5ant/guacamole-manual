<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE refentry PUBLIC "-//OASIS//DTD DocBook V5.0//EN"
    "http://docbook.org/xml/5.0/dtd/docbook.dtd">

<refentry id="clipboard" 
    xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
    xmlns:xi="http://www.w3.org/2001/XInclude">

    <refmeta>
        <refentrytitle>clipboard</refentrytitle>
    </refmeta>

    <refnamediv>
        <refname>clipboard</refname>
        <refpurpose>Sets the client or server clipboard to the given data.</refpurpose>
    </refnamediv>

    <refsynopsisdiv>

        <title>Synopsis</title>

        <para>
            <command>
                clipboard
                <parameter>DATA</parameter>
            </command>
        </para>

    </refsynopsisdiv>

    <refsection>
        <title>Description</title>
        <para>Sets the client or server clipboard to the given data. Both the client and server can send this instruction.</para>
    </refsection>

    <refsection>

        <title>Example</title>

        <programlisting>[example]</programlisting>

    </refsection>

</refentry>