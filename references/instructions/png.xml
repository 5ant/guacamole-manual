    <refmeta>
        <refentrytitle>png</refentrytitle>
    </refmeta>

    <refnamediv>
        <refname>png</refname>
        <refpurpose>Loads PNG data into the specified location of the specified layer or buffer.</refpurpose>
    </refnamediv>

    <refsynopsisdiv>

        <title>Synopsis</title>

        <para>
            <command>
                png
                <parameter>MASK</parameter>
                <parameter>LAYER</parameter>
                <parameter>X</parameter>
                <parameter>Y</parameter>
                <parameter>DATA</parameter>
            </command>
        </para>

    </refsynopsisdiv>

    <refsection>
        <title>Description</title>
        <para>Loads PNG data into the specified location of the specified layer or buffer.</para>
    </refsection>

    <refsection>

        <title>Parameters</title>

        <variablelist>
            <varlistentry>
