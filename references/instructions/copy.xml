<refentry id="copy-instruction" 
    xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
    xmlns:xi="http://www.w3.org/2001/XInclude">

    <indexterm>
        <primary>copy</primary>
    </indexterm>

    <refmeta>
        <refentrytitle>copy</refentrytitle>
    </refmeta>

    <refnamediv>
        <refname>copy</refname>
        <refpurpose>Copies image data from the specified rectangle of the specified layer or buffer to a different location of another specified layer or buffer.</refpurpose>
    </refnamediv>

    <refsynopsisdiv>

        <title>Synopsis</title>

        <para>
            <command>
                copy
                <parameter>SRCLAYER</parameter>
                <parameter>SRCX</parameter>
                <parameter>SRCY</parameter>
                <parameter>SRCWIDTH</parameter>
                <parameter>SRCHEIGHT</parameter>
                <parameter>MASK</parameter>
                <parameter>DSTLAYER</parameter>
                <parameter>DSTX</parameter>
                <parameter>DSTY</parameter>
            </command>
        </para>

    </refsynopsisdiv>

    <refsection>
        <title>Description</title>
        <para>Copies image data from the specified rectangle of the specified layer or buffer to a different location of another specified layer or buffer.</para>
    </refsection>

    <refsection>

        <title>Example</title>

        <programlisting>[example]</programlisting>

    </refsection>

</refentry>