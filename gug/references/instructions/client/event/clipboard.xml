<?xml version="1.0" encoding="UTF-8"?>

<section xml:id="client-clipboard-instruction" 
    xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
    xmlns:xi="http://www.w3.org/2001/XInclude">



    <title>clipboard</title>
    <indexterm>
        <primary>clipboard</primary>
    </indexterm>
    <para>Sets the client or server clipboard to the given data. Both the client
        and server can send this instruction.</para>
    <variablelist>
        <varlistentry>
            <term><parameter>data</parameter></term>
            <listitem>
                <para>The data the clipboard should be set to.</para>
            </listitem>
        </varlistentry>
    </variablelist>

</section>