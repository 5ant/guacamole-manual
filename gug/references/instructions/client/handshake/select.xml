<?xml version="1.0" encoding="UTF-8"?>

<section xml:id="select-instruction" 
    xmlns="http://docbook.org/ns/docbook" version="5.0" xml:lang="en"
    xmlns:xi="http://www.w3.org/2001/XInclude">
    <title>select</title>

    <indexterm>
        <primary>select</primary>
    </indexterm>
    <para>Requests that the connection be made using the specified protocol.
        This is the first instruction sent during the handshake phase.</para>
    <variablelist>
        <varlistentry>
            <term><parameter>protocol</parameter></term>
            <listitem>
                <para>The protocol to use.</para>
            </listitem>
        </varlistentry>
    </variablelist>

</section>